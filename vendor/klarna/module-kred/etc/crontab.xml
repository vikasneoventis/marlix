<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Cron:etc/crontab.xsd">
    <group id="default">
        <job name="klarna_kco_clean_claimed_pushqueues" instance="Klarna\Kred\Model\CronJob\CleanClaimedPushqueues" method="execute">
            <schedule>0 * * * *</schedule>
        </job>
        <job name="klarna_kco_clean_expired_pushqueues" instance="Klarna\Kred\Model\CronJob\CleanExpiredPushqueues" method="execute">
            <schedule>0 * * * *</schedule>
        </job>
    </group>
</config>
