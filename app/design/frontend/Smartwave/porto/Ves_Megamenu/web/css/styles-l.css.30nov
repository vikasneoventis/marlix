.page-header .navigation .level0.parent > a::after {
    display: inline-block;
    font-family: 'porto-icons';
    vertical-align: middle;
    margin-left: 10px;
    content: '\f813';
    line-height: 42px;
}



.selected_pro{ min-height:415px;}

.selected_pro .product-item-name {
    min-height: 50px;
}

.selected_pro .products {
    margin: 0px 0;
}

.masonry-grid-item .selected_pro a:before {
    width: 100%;
    height: 100%;
    display: block;
    content: "";
    position: absolute;
    visibility: hidden;
    opacity: 0;
    filter: alpha(opacity=0);
    background-color:transparent !important;
    transition: 0.3s all;
}

/*.products-grid:not(.side-list):not(.products-related) .product-item .product-item-details {
    padding: 53px 10px 10px !important;
}*/
.selected_pro .owl-middle-outer-narrow .owl-theme .owl-controls .owl-nav .owl-prev {
    left: 0px;
}
.selected_pro .owl-middle-outer-narrow .owl-theme .owl-controls .owl-nav .owl-next {
    right: 0px;
}





.page-header .navigation .level0 .submenu {
    padding: 0px !important;
}
.ves-megamenu .navigation .nav-item.subhover>.submenu>.submenu-inner {
    padding: 0px 15px !important;
}

.ves-megamenu .navigation .level0 .submenu .submenu-inner .content-wrapper {
    float: left;
    width: 100%;
    background-size: contain !important;
    background-position-y: top !important;
    background-position-x: right !important;
	min-height:350px;
}

.ves-megamenu .navigation .nav-item.subhover>.submenu>.submenu-inner .submenu{
	min-height:100%;
}

.ves-megamenu .navigation .nav-item.subhover>.submenu>.submenu-inner .submenu{
    padding: 13px !important;
}

.ves-megamenu .navigation .nav-item.subhover > .submenu > .submenu-inner .submenu {
    min-height: 100%;
    position: absolute;
    left: 180px !important;
    top: 10px !important;
}

.owl-dots {
    margin-top: 30px;
